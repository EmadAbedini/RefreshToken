generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  id         Int @id @default(autoincrement())
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt

  username String @unique
  password String
  hashedRT String? // RT = Refresh Token

  @@map("users")
}
